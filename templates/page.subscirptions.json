    "6024b511-3b5f-4b58-bf9e-e87035ad881f": {
      "type": "faq",
      "blocks": {
        "template--16912097968341__6024b511-3b5f-4b58-bf9e-e87035ad881f-question-1": {
          "type": "question",
          "settings": {
            "title": "How do I contact customer support for subscription-related inquiries?",
            "text": "<p>You can reach our customer support team via email at customerservice@beautystat.com. We're here to help with any subscription questions or issues.<\/p>"
          }
        },
        "template--16912097968341__6024b511-3b5f-4b58-bf9e-e87035ad881f-question-2": {
          "type": "question",
          "settings": {
            "title": "What is the return policy for subscription products?",
            "text": "<p>Subscription products can be returned within our standard return window. Please review our return policy for more information.<\/p>"
          }
        },
        "b7ac45b8-f65c-4dba-ad02-b06ea7be8774": {
          "type": "question",
          "settings": {
            "title": "Can I use discount codes or promotions on subscription orders?",
            "text": "<p>Only in specifically defined promos can discount codes be used for an additional saving on the first order of a subscription. Subscriptions automatically enjoy a 15% discount on recurring shipments. <\/p>"
          }
        },
        "6371928a-8335-4e9e-8418-c7228d76cf41": {
          "type": "question",
          "settings": {
            "title": "What happens if my credit card expires or payment fails for a subscription order?",
            "text": "<p>You will receive a notification to update your payment information. If not updated, your subscription will be paused until the issue is resolved.<\/p>"
          }
        },
        "3406ec79-b040-4c91-825f-5e79ee646e1f": {
          "type": "question",
          "settings": {
            "title": "Can I subscribe to international shipping for BeautyStat products?",
            "text": "<p>Currently, our subscription service is available for customers in the United States only.<\/p>"
          }
        },
        "4eaf6cc7-103f-49a7-a494-a6170bb9e2b9": {
          "type": "question",
          "settings": {
            "title": "How do I update my shipping address or payment information?",
            "text": "<ul><li>You can update your shipping address or payment details in your account settings under the \"Subscription\" tab.<\/li><\/ul>"
          }
        }
      },
